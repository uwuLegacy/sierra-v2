{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/framework/src",
  "monorepo": true,
  "root": "apps/framework",
  "compilerOptions": {
    "tsConfigPath": "apps/framework/tsconfig.app.json",
    "webpack": false
  },
  "projects": {
    "framework": {
      "type": "application",
      "root": "apps/framework",
      "entryFile": "main",
      "sourceRoot": "apps/framework/src",
      "compilerOptions": {
        "tsConfigPath": "apps/framework/tsconfig.app.json"
      }
    },
    "client": {
      "type": "library",
      "root": "libs/client",
      "entryFile": "index",
      "sourceRoot": "libs/client/src",
      "compilerOptions": {
        "tsConfigPath": "libs/client/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "structures": {
      "type": "library",
      "root": "libs/structures",
      "entryFile": "index",
      "sourceRoot": "libs/structures/src",
      "compilerOptions": {
        "tsConfigPath": "libs/structures/tsconfig.lib.json"
      }
    },
    "prisma": {
      "type": "library",
      "root": "libs/prisma",
      "entryFile": "index",
      "sourceRoot": "libs/prisma/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prisma/tsconfig.lib.json"
      }
    },
    "modules": {
      "type": "library",
      "root": "libs/modules",
      "entryFile": "index",
      "sourceRoot": "libs/modules/src",
      "compilerOptions": {
        "tsConfigPath": "libs/modules/tsconfig.lib.json"
      }
    }
  }
}